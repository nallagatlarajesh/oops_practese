class VivoMobile:
    def __init__(self):
        print("welcome to Vivo mobiles")

    def info(self):
        print("x50","16gb","256gb")

    def example(self):
        print("example is working....")
#we create objects
mob1=VivoMobile()

#accesing the attributes
mob1.info()

mob2=VivoMobile()
mob2.info()

#accesing the attributes
#mob1.example()
